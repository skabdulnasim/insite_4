= form_for @product_attribute_set do |f|
  - if @product_attribute_set.errors.any?
    #error_explanation
      %h2= "#{pluralize(@product_attribute_set.errors.count, "error")} prohibited this product_attribute_set from being saved:"
      %ul
        - @product_attribute_set.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :is_default
    = f.check_box :is_default
  .field
    = f.label :deleted_at
    = f.datetime_select :deleted_at
  .actions
    = f.submit 'Save'
