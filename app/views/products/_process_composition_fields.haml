%tr{:id=>"cart_item_process_#{f.object.production_process_id}"}
	%td
		= f.text_field :production_process_name, :readonly=>true, :style=>"color:#000; border-bottom:none;"
		= f.hidden_field :production_process_id
	%td
		= f.text_field :duration
	%td
		= "(Duration in minutes)"
	%td
		= f.hidden_field :_destroy
		= link_to "Remove", "#", class: "remove_processes", data:{"process-id"=>"#{f.object.production_process_id}"}