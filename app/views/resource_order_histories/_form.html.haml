= form_for @resource_order_history do |f|
  - if @resource_order_history.errors.any?
    #error_explanation
      %h2= "#{pluralize(@resource_order_history.errors.count, "error")} prohibited this resource_order_history from being saved:"
      %ul
        - @resource_order_history.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :resource_id
    = f.number_field :resource_id
  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :unit_id
    = f.number_field :unit_id
  .field
    = f.label :recorded_at
    = f.datetime_select :recorded_at
  .field
    = f.label :latitude
    = f.text_field :latitude
  .field
    = f.label :longitude
    = f.text_field :longitude
  .actions
    = f.submit 'Save'
