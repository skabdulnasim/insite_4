= form_for @medium do |f|
  - if @medium.errors.any?
    #error_explanation
      %h2= "#{pluralize(@medium.errors.count, "error")} prohibited this medium from being saved:"
      %ul
        - @medium.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :media_file_name
    = f.text_field :media_file_name
  .field
    = f.label :media_content_type
    = f.text_field :media_content_type
  .field
    = f.label :media_file_size
    = f.text_field :media_file_size
  .field
    = f.label :media_updated_at
    = f.datetime_select :media_updated_at
  .field
    = f.label :asset_id
    = f.number_field :asset_id
  .field
    = f.label :asset_type
    = f.text_field :asset_type
  .field
    = f.label :properties
    = f.text_area :properties
  .field
    = f.label :deleted_at
    = f.datetime_select :deleted_at
  .actions
    = f.submit 'Save'
