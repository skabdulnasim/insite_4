= form_for @resource_order_history_detail do |f|
  - if @resource_order_history_detail.errors.any?
    #error_explanation
      %h2= "#{pluralize(@resource_order_history_detail.errors.count, "error")} prohibited this resource_order_history_detail from being saved:"
      %ul
        - @resource_order_history_detail.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :menu_product_id
    = f.number_field :menu_product_id
  .field
    = f.label :product_id
    = f.number_field :product_id
  .field
    = f.label :product_name
    = f.text_field :product_name
  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :unit_id
    = f.number_field :unit_id
  .field
    = f.label :recorded_at
    = f.datetime_select :recorded_at
  .field
    = f.label :remrks
    = f.text_area :remrks
  .actions
    = f.submit 'Save'
