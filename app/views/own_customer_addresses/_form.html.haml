= form_for @own_customer_address do |f|
  - if @own_customer_address.errors.any?
    #error_explanation
      %h2= "#{pluralize(@own_customer_address.errors.count, "error")} prohibited this own_customer_address from being saved:"
      %ul
        - @own_customer_address.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :contact_no
    = f.text_field :contact_no
  .field
    = f.label :pincode
    = f.text_field :pincode
  .field
    = f.label :landmark
    = f.text_field :landmark
  .field
    = f.label :city
    = f.text_field :city
  .field
    = f.label :state
    = f.text_field :state
  .field
    = f.label :latitude
    = f.text_field :latitude
  .field
    = f.label :longitude
    = f.text_field :longitude
  .actions
    = f.submit 'Save'
