= form_for([@store_rack.store, @store_rack]) do |f|
  - if @store_rack.errors.any?
    #error_explanation
      %h2= "#{pluralize(@store_rack.errors.count, "error")} prohibited this store_rack from being saved:"
      %ul
        - @store_rack.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :height
    = f.text_field :height
  .field
    = f.label :width
    = f.text_field :width
  .actions
    = f.submit 'Save'
