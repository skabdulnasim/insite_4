= form_for @resource_product_stock do |f|
  - if @resource_product_stock.errors.any?
    #error_explanation
      %h2= "#{pluralize(@resource_product_stock.errors.count, "error")} prohibited this resource_product_stock from being saved:"
      %ul
        - @resource_product_stock.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :resource_id
    = f.number_field :resource_id
  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :product_id
    = f.number_field :product_id
  .field
    = f.label :stock
    = f.text_field :stock
  .field
    = f.label :recorded_at
    = f.datetime_select :recorded_at
  .actions
    = f.submit 'Save'
