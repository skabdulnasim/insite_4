<table class="table table-hover table-bordered">
	<thead>
		<tr class="table-header well">
			<th class="col-lg-3">Product Name</th>
			<th class="col-lg-3">Total No of Product</th>
			<th class="col-lg-3">Total Revenue</th>
			<th class="col-lg-3">Current Stock</th>
		</tr>
	</thead>
	
	<tbody>		
		<% for(var i=0; i<product_details.length; i++) { %>
			<tr>
				<td class="col-lg-3"><%= product_details[i].name %></td>
				<td class="col-lg-3"><%= product_details[i].no_of_product %></td>
				<td class="col-lg-3"><%= product_details[i].total_price %></td>
				<td class="col-lg-3"><%= product_details[i].current_stock %><%= product_details[i].basic_unit %></td>
			</tr>	
		<% } %>		
	</tbody>
</table>
