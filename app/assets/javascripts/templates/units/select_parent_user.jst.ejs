<div class="filter-title margin-t-b-0">
  <i class="mdi-action-label left"></i>
  Parent User
</div>
<div class="padding-5">
  <select class="form-control" onChange="setParent(this)" id="parent_user_id" class="select optional form-control" name="parent_user_id" required="required">
