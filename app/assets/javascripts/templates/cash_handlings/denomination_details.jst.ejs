<div class="col-lg-12 padding-5">
  <div class="card waves-effect waves-light">
    <div class="card-content p0">
      <p class="header white-text teal padding-10">
        DENOMINATION DETAILS
        <b class="white-text float-r">
	        <% if(responseData.data.transaction.bill_serial_no != null) { %>
	        	Bill Serial No: <%=responseData.data.transaction.bill_serial_no%> &nbsp;
	        <% } %>          
	        <% if(responseData.data.transaction.device_id != null) { %>
        		Device Id: <%=responseData.data.transaction.device_id%> &nbsp;
        	<% } %> 
        	Recorded At: <%=formatDateTime(responseData.data.transaction.recorded_at)%>
        </b>
      </p>
    </div>
    <% if(typeof(responseData.data.transaction.description) !== 'undefined'){%>
	    <% 
	    	var response_data = responseData.data.transaction.description.length ;
	    %>
	    <% if (response_data > 0) { %>
		    <% for (var i = response_data - 1; i >= 0; i--) { %> 
		    	<div class="col-lg-3 padding-5">
		    		<div class="padding-5 card">
		    			<img src="<%= responseData.data.transaction.description[i].image %>" id="rupee_<%= responseData.data.transaction.id%>" class="denomination_details_<%= responseData.data.id%>" style= "width:100px; height:50px;" data-count="<%= responseData.data.transaction.description[i].count%>" data-value="<%= responseData.data.transaction.description[i].value%>"/>
		    			<span class="m-btn green" id="green_button" style= "text-align:center" >
		    			<%= responseData.data.transaction.description[i].count %></span>
		    		</div>
		    	</div>
		    <%}%>
			<% } else{ %>
				<div calss="no_description">
					Cash description is not present 
				</div>	
		  <% } %>
		<%}else{%>  
			<div calss="no_description">
				Cash description is not present 
			</div>	
		<%}%> 
	</div>
</div>



